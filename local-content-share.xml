<?xml version="1.0"?>
<Container version="2">
  <Name>local-content-share</Name>
  <Repository>tanq16/local-content-share:main</Repository>
  <Registry>docker.io</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>Local sharing of text snippets, code, and files via a PWA interface.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/Tanq16/local-content-share/blob/main/assets/logo.png?raw=true</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1748400202</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="APP_DATA" Target="/app/data" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/local-content-share</Config>
  <Config Name="PORT" Target="8080" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">9999</Config>
  <TailscaleStateDir/>
</Container>
